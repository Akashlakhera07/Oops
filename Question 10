#include<iostream>
using namespace std;
class student{
  public:
    int roll_no;
    char section;
    void display() {
      cout<<"AKASH LAKHERA"<<endl;
      cout<<roll_no<<endl;
      cout<<section<<endl;
    }
    void set_data(){
      roll_no=43;
      section = 'A';
    }
};
int main(){
  student s1;
  s1.set_data();
  s1.display();
  return 0;
}
